{
  "trailingComma": "none",
  "tabWidth": 4,
  "useTabs": true,
  "semi": true,
  "singleQuote": false,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "/routes/",
    "/middleware/",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}